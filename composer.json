{
  "name": "telabook/shared-amqp",
  "description": "This package enable amqp for microseervices",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Telabook\\Amqp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Telabook\\Amqp\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "dev": "php index.php",
    "test": "vendor/bin/phpunit tests",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes --config=.php_cs.dist.php"
  },
  "authors": [
    {
      "name": "Wasif Farooq",
      "email": "wasif0332@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "mateusjunges/laravel-kafka": "v1.13.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^7.16|^8.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  }
}
